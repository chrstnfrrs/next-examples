{
  "name": "tailwind-emotion",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "build:static": "next build && next export",
    "prettier": "prettier --write .",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet",
